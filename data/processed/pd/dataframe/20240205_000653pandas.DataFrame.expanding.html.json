"{   \"name\": \"DataFrame.expanding\",   \"api_name\": \"DataFrame.expanding\",   \"description\": \"Provide expanding window calculations.\",   \"parameters\": {     \"required\": [       {         \"name\": \"min_periods\",         \"description\": \"Minimum number of observations in window required to have a value; otherwise, result is np.nan.\"       }     ],     \"optional\": [       {         \"name\": \"axis\",         \"description\": \"If 0 or 'index', roll across the rows. If 1 or 'columns', roll across the columns. For Series this parameter is unused and defaults to 0.\"       },       {         \"name\": \"method\",         \"description\": \"Execute the rolling operation per single column or row ('single') or over the entire object ('table'). This argument is only implemented when specifying engine='numba' in the method call.\",         \"default_value\": \"'single'\",         \"new_in_version\": \"1.3.0\"       }     ]   } }"