"{   \"name\": \"pandas.DataFrame.insert\",   \"api_name\": \"pandas.DataFrame.insert\",   \"description\": \"Insert column into DataFrame at specified location. Raises a ValueError if column is already contained in the DataFrame, unless allow_duplicates is set to True.\",   \"parameters\": {     \"required\": [       {         \"name\": \"loc\",         \"description\": \"Insertion index. Must verify 0 <= loc <= len(columns).\"       },       {         \"name\": \"column\",         \"description\": \"Label of the inserted column.\"       },       {         \"name\": \"value\",         \"description\": \"Content of the inserted column.\"       }     ],     \"optional\": [       {         \"name\": \"allow_duplicates\",         \"description\": \"Allow duplicate column labels to be created.\"       }     ]   } }"